<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="bibliotecamodel">
    <class name = "Imprumut" table="Imprumut">
        <id name = "id" column = "id">
            <generator class="increment"/>
        </id>
        <property name="idCarte" column = "idCarte"/>
        <property name="idAbonat" column="idAbonat"/>
        <property name="dataImprumut" type="date" column="dataImprumut"/>
        <property name="returnat" column="returnat"/>
    </class>

</hibernate-mapping>